{
  "name": "eatlas",
  "private": true,
  "version": "1.0.0",
  "description": "Scripts common to client & server (mainly for easier dev)",
  "main": "server/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "cd client && yarn start",
    "dev:server": "cd server && yarn dev",
    "dev:sass": "cd client && yarn sass",
    "test": "run-p test:*",
    "test:client": "cd client && yarn test",
    "test:server": "cd server && yarn test",
    "postinstall": "run-p install:*",
    "install:client": "cd client && yarn install",
    "install:server": "cd server && yarn install",
    "add-user": "cd server && yarn add-user",
    "check-files": "cd server && yarn check-files",
    "es-index": "cd server && yarn es-index",
    "build": "cd client && yarn build",
    "start": "cd server && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byteclubfr/eatlas.git"
  },
  "author": "Nicolas Chambrier <naholyr@gmail.com> (http://naholyr.fr/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byteclubfr/eatlas/issues"
  },
  "homepage": "https://github.com/byteclubfr/eatlas#readme",
  "dependencies": {
    "npm-run-all": "^4.1.2"
  },
  "devDependencies": {}
}
