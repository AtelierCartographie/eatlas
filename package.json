{
  "name": "eatlas",
  "private": true,
  "version": "1.0.0",
  "description": "Scripts common to client & server (mainly for easier dev)",
  "main": "server/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && npm run dev",
    "test": "run-p test:*",
    "test:client": "cd client && npm test",
    "test:server": "cd server && npm test",
    "postinstall": "run-p install:*",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byteclubfr/eatlas.git"
  },
  "author": "Nicolas Chambrier <naholyr@gmail.com> (http://naholyr.fr/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byteclubfr/eatlas/issues"
  },
  "homepage": "https://github.com/byteclubfr/eatlas#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {}
}
